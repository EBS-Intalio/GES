<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_account_payment_receive" model="ir.sequence">
            <field name="name">PAYMENT RECEIVE</field>
            <field name="code">account.payment.sequence.receive</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
            <field name="active" eval="True"/>
        </record>
        <record id="sequence_account_payment_send" model="ir.sequence">
            <field name="name">PAYMENT SEND</field>
            <field name="code">account.payment.sequence.send</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
            <field name="active" eval="True"/>
        </record>
    </data>
    <data>
        <record id="account_payment_method_check_in" model="account.payment.method">
            <field name="name">Checks</field>
            <field name="code">check</field>
            <field name="payment_type">inbound</field>
        </record>
    </data>
</odoo>
