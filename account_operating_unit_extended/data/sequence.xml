<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="sequence_account_move" model="ir.sequence">
            <field name="name">INVOICE</field>
            <field name="code">account.move.sequence.updated</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
            <field name="active" eval="True"/>
        </record>

        <record id="sequence_account_move_bill" model="ir.sequence">
            <field name="name">BILL</field>
            <field name="code">account.move.sequence.bill</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
            <field name="active" eval="True"/>
        </record>

        <record id="sequence_account_move_entry" model="ir.sequence">
            <field name="name">JOURNAL ENTRY</field>
            <field name="code">account.move.sequence.entry</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
            <field name="active" eval="True"/>
        </record>

    </data>
</odoo>
