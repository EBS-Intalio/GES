                                            <field name="source"/>
                                            <field name="job_type"/>
                                            <field name="direction"/>
                                            <field name="transport"/>
                                            <field name="currency_id"/>
                                            <field name="cfx"/>
                                        </tree>
                                    </field>
                                </group>
