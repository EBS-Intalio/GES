                        <field name="res_addr"/>
                        <field name="mailing_addr_sales_update"/>
                    </group>


                </group>
                <notebook>
                    <page string="Loading/Unloading Constraints" name="load_unload_constraints">
                        <group>
                            <group>
                                <field name="access_point"/>
                                <field name="communication"/>
                                <field name="dock_height"/>
                                <label for="service_duration" string="Service Duration(minutes):" style="font-weight:bold" />
                                <br/>
                                <label for="service_duration_default" string="Override Registry Default" style="font-weight:bold"/>
                                <div>
                                    <field name="service_duration_default" nolabel="1" class="oe_inline" style="width:10%"/>
                                    <field name="service_duration" nolabel="1" style="width:20%" class="oe_inline"/>

                                </div>

                            </group>
                            <group>
                                <field name="container_handling"/>
                                <field name="labour_required"/>
                                <field name="reports_own_vgm"/>
                            </group>
                        </group>
                        <group>
                        <field name="further_constraints" string="Further Constraints" /></group>
                    </page>
                    <page string="Warehouse Equipment" name="warehouse_equipment">
                        <group style="width:100%">
                            <group style="width:33%">
                                <field name="has_dock_leveler"/>
                            </group>
                            <group style="width:33%">
                                <field name="has_pallect_jack"/>
                            </group>
                            <group style="width:33%">
                                <field name="has_fork_lift"/>
                            </group>
                        </group>
                        <group>
                        <field name="other_warehouse_fac"/></group>
                    </page>
                    <page string="Drop Mode" name="drop_mode">
                        <group string="Port Transport Drop Mode">
                            <field name="air_freight" style="width:33%"/>
                            <field name="lcl_freight" style="width:33%"/>
                            <field name="fcl_freight" style="width:33%"/>
                        </group>
                    </page>
                    <page string="Pickup &amp; Delivery" name="pickup_delivery">
                        <group string="Delivery">
                            <field name="auth_to_leave" style="width:33%"/>
                        </group>
                        <group string='Pickup and Delivery Times'>
                            <field name="pickup_and_delivery_times" widget="radio" options="{'horizontal': true}"/>
                            <field name="pickup_delivery_ids">
                                <tree editable="bottom">
                                    <field name="type"/>
                                    <field name="desc"/>
                                    <field name="from_date"/>
                                    <field name="to_date"/>
                                    <field name="day"/>
                                </tree>
                            </field>
                        </group>
                    </page>
                    <page string="GPS" name="gps">
                        <group string="Coordinates">
                            <field name="longitude" style="width:33%"/>
                            <field name="latitude" style="width:33%"/>
                        </group>
                    </page>
                    <page string="Delivery Route" name="delivery_route">
                        <group>
                            <field name="delivery_route" style="width:33%"/>
                            <field name="route_seq" style="width:33%"/>
                        </group>
                    </page>
                    <page string="Free Waiting" name="free_waiting">
                        <group>
                            <field name="use_cumulative" style="width:33%"/>
                            <field name="free_waiting_ids">
                                <tree editable="bottom">
                                    <field name="partner_id" invisible="1"/>
                                    <field name="container_type_id"/>
                                    <field name="drop_mode"/>
                                    <field name="cfs"/>
                                    <field name="cne"/>
                                    <field name="cnr"/>
                                    <field name="cto"/>
                                    <field name="cyd"/>
                                    <field name="other"/>
                                </tree>
                            </field>
                        </group>
                    </page>
                </notebook>
            </xpath>

        </field>
    </record>
    <record id="view_partner_form_inherit_freight_inherit" model="ir.ui.view">
            <field name="name">res.partner.form.inherit_new.inherit</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="freight.view_partner_form_inherit_freight"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='shipper']" position="replace">
                </xpath>
                <xpath expr="//field[@name='consignee']" position="replace">
                </xpath>
            </field>
        </record>
</odoo>
