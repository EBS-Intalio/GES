<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="freight_management_res_partner_form_view" model="ir.ui.view">
        <field name="name">freight_management.res.partner.form.view</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <field name="category_id" position="after">
                <field name="freight_type" invisible="1"/>
<!--                <field name="is_carrier"/>-->
            </field>
            <field name="vat" position="after">
                <field name="organization_code" required="1"/>
                <field name="screening" required="1"/>
                <field name="security"/>
                <field name="unloco"/>
                <field name="org_category"/>
                <field name="controlling_branch"/>
                <field name="lang_main"/>
                <field name="vat_ges" widget="selection"/>
            </field>
            <field name="street" position="attributes">
                <attribute name="required">1</attribute>
            </field>
            <field name="street2" position="attributes">
                <attribute name="required">1</attribute>
            </field>
            <field name="city" position="attributes">
                <attribute name="required">1</attribute>
            </field>
            <field name="state_id" position="attributes">
                <attribute name="required">1</attribute>
            </field>
            <field name="country_id" position="attributes">
                <attribute name="required">1</attribute>
            </field>
            <field name="zip" position="attributes">
                <attribute name="required">1</attribute>
            </field>

	    <field name="phone" position="before">
                <field nolabel="1" name="country_image_url" widget="image_url" class="oe_avatar_flag"/>
            </field>

            <field name="mobile" position="before">
                <field nolabel="1" name="country_image_url" widget="image_url" class="oe_avatar_flag"/>
            </field>

            <xpath expr="//form/sheet/group" position="inside">
                <group name="organization_type" string="Organization Type" style="width:100%;">
                    <group style="width:33%">
                        <field name="is_national_account" attrs="{'invisible':[('is_global_supplier','=',True)]}"/>
                        <field name="is_global_supplier" attrs="{'invisible':[('is_national_account','=',True)]}"/>
                        <field name="is_temporary_act"/>
                        <field name="is_receivable"/>
                        <field name="is_payable"/>
                        <field name="shipper"/>

                    </group>
                    <group style="width:33%">
                        <field name="consignee"/>
                        <field name="is_transport_client"/>
                        <field name="is_warehouse"/>
                        <field name="is_carrier"/>
                        <field name="is_forwarder_Agent"/>
                        <field name="is_broker"/>

                    </group>
                    <group style="width:33%">
                        <field name="is_services"/>
                        <field name="is_competitor"/>
                        <field name="is_sales"/>
                        <field name="is_controlling_agent"/>
                    </group>


                </group>
            </xpath>
