<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_bookings" model="ir.sequence">
            <field name="name">Freight Request</field>
            <field name="code">freight.job.request</field>
            <field name="prefix">FR/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_freight_operation" model="ir.sequence">
            <field name="name">SHIP</field>
            <field name="code">freight.operation</field>
            <field name="prefix">SHIP/%(year)s/%(month)s/%(day)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="consol_details_sequence" model="ir.sequence">
            <field name="name">Console Details</field>
            <field name="code">consol.details</field>
            <field name="prefix">CD</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
