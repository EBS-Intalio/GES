<odoo>
    <data>
        <!--                 menu="False"-->
        <report
                id="fright_account_invoice_shipment_pdf"
                model="account.move"
                string="Shipment Invoice"
                name="freight_account_shipment_pdf.invoice_shipment_pdf"
                file="freight_account_shipment_pdf.invoice_shipment_pdf"
                report_type="qweb-pdf"
        />

        <record id="paper_format_invoice_shipment" model="report.paperformat">
            <field name="name">paper format invoice shipment</field>
            <field name="default" eval="True"/>
            <field name="format">A4</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">5</field>
            <field name="margin_bottom">0</field>
            <field name="margin_left">10</field>
            <field name="margin_right">10</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">0</field>
        </record>
        <record id="freight_account_shipment_pdf.fright_account_invoice_shipment_pdf" model="ir.actions.report">
            <field name="paperformat_id" ref="freight_account_shipment_pdf.paper_format_invoice_shipment"/>
        </record>

    </data>
</odoo>